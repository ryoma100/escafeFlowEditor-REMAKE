{
  "name": "escafe-flow-editor-remake",
  "version": "0.0.0",
  "description": "escafeFlowEditor REMAKE",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "lint": "run-p lint:* --silent",
    "lint:cspell": "cspell --no-progress --no-summary '**'",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier . --check --log-level warn",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "eslint --fix",
    "lint-fix:prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "storycap": "storycap --delay 1000 http://localhost:6006",
    "build-storybook": "storybook build",
    "build-storycap": "storycap --serverTimeout 60000 --captureTimeout 10000 --serverCmd \"npx http-server storybook-static -p 9003\" http://localhost:9003",
    "vrt-before": "rm -rf __local-vrt__/before; storycap -o __local-vrt__/before --delay 1000 http://localhost:6006",
    "vrt-after": "rm -rf __local-vrt__/after; storycap -o __local-vrt__/after --delay 1000 http://localhost:6006",
    "vrt-diff": "rm -rf __local-vrt__/diff; reg-cli __local-vrt__/after __local-vrt__/before __local-vrt__/diff -R __local-vrt__/report.html; open __local-vrt__/report.html",
    "preinstall": "npx -y only-allow pnpm"
  },
  "license": "MIT",
  "dependencies": {
    "@solid-primitives/i18n": "^2.2.0",
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/plugin-dialog": "~2.2.0",
    "@tauri-apps/plugin-fs": "~2.2.0",
    "@tauri-apps/plugin-process": "~2.2.0",
    "@tauri-apps/plugin-shell": "~2.2.0",
    "fast-xml-parser": "^4.5.1",
    "solid-js": "^1.9.4",
    "valibot": "1.0.0-beta.14"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-links": "^8.5.1",
    "@storybook/addon-themes": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/builder-vite": "^8.5.1",
    "@storybook/docs-tools": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@tauri-apps/cli": "^2.2.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/coverage-v8": "^3.0.4",
    "cspell": "^8.17.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "lefthook": "^1.10.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "reg-cli": "^0.18.10",
    "solid-devtools": "^0.33.0",
    "storybook": "^8.5.1",
    "storybook-solidjs": "1.0.0-beta.6",
    "storybook-solidjs-vite": "1.0.0-beta.6",
    "storycap": "^5.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-plugin-solid": "^2.11.0",
    "vite-plugin-solid-svg": "^0.8.1",
    "vitest": "^3.0.4"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.15.4"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}
