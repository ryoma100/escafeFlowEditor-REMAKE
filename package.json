    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.10.0",
    "jsdom": "^25.0.1",
    "lefthook": "^1.7.18",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "reg-suit": "^0.14.4",
    "solid-devtools": "^0.30.1",
    "storybook": "^8.3.5",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "storycap": "^5.0.1",
