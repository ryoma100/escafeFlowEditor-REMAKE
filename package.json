{
  "name": "tiny-es-flow-editor",
  "version": "0.0.0",
  "description": "tiny esFlow Diagram Editor",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "lint": "run-s -c lint:*",
    "lint:cspell": "cspell --no-progress '**'",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:prettier": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "lint:stylelint": "stylelint '*/*.css'",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "lint-fix:stylelint": "stylelint \"**/*.css\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preinstall": "npx -y only-allow pnpm"
  },
  "license": "MIT",
  "dependencies": {
    "@solid-primitives/i18n": "^2.0.1",
    "@tauri-apps/api": "^1.5.3",
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.1.9",
    "@storybook/addon-essentials": "8.0.0-beta.2",
    "@storybook/addon-interactions": "8.0.0-beta.2",
    "@storybook/addon-links": "8.0.0-beta.2",
    "@storybook/blocks": "8.0.0-beta.2",
    "@storybook/testing-library": "^0.2.2",
    "@tauri-apps/cli": "^1.5.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-solid": "^0.13.1",
    "eslint-plugin-storybook": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "solid-devtools": "^0.29.3",
    "storybook": "8.0.0-beta.2",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-solid": "^2.10.1",
    "vite-plugin-solid-svg": "^0.8.0"
  },
  "volta": {
    "node": "20.11.1",
    "pnpm": "8.15.3"
  }
}
