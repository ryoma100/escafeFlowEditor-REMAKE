{
  "name": "escafe-flow-editor-remake",
  "version": "0.0.0",
  "description": "escafeFlowEditor REMAKE",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "lint": "run-p lint:* --silent",
    "lint:cspell": "cspell --no-progress --no-summary '**'",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint",
    "lint:biome": "biome check --diagnostic-level=warn",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "eslint --fix",
    "lint-fix:biome": "biome check --diagnostic-level=warn --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storycap": "storycap --serverTimeout 60000 --captureTimeout 10000 --serverCmd \"npx http-server storybook-static -p 9003\" http://localhost:9003",
    "vrt-before": "mkdir -p __screenshots__; cd $_; rm -rf before; storycap -o before --delay 1000 http://localhost:6006",
    "vrt-after": "mkdir -p __screenshots__; cd $_; rm -rf after; storycap -o after --delay 1000 http://localhost:6006",
    "vrt-diff": "cd __screenshots__; rm -rf diff; reg-cli after before diff -R report.html; open report.html",
    "preinstall": "npx -y only-allow pnpm"
  },
  "license": "MIT",
  "dependencies": {
    "@corvu/resizable": "^0.2.4",
    "@solid-primitives/i18n": "^2.2.0",
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/plugin-dialog": "~2.2.0",
    "@tauri-apps/plugin-fs": "~2.2.0",
    "@tauri-apps/plugin-process": "~2.2.0",
    "@tauri-apps/plugin-shell": "~2.2.0",
    "fast-xml-parser": "^4.5.1",
    "solid-js": "^1.9.4",
    "valibot": "1.0.0-beta.15"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-themes": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/builder-vite": "^8.5.3",
    "@storybook/docs-tools": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@tailwindcss/vite": "^4.0.6",
    "@tauri-apps/cli": "^2.2.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/coverage-v8": "^3.0.5",
    "cspell": "^8.17.3",
    "eslint": "^9.20.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "lefthook": "^1.10.10",
    "npm-run-all": "^4.1.5",
    "reg-cli": "^0.18.10",
    "solid-devtools": "^0.33.0",
    "storybook": "^8.5.3",
    "storybook-solidjs": "1.0.0-beta.6",
    "storybook-solidjs-vite": "1.0.0-beta.6",
    "storycap": "^5.0.1",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-solid": "^2.11.1",
    "vite-plugin-solid-svg": "^0.8.1",
    "vitest": "^3.0.5"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "10.3.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}
